{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 40,
      "minLength": 3
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    }
  }
}
